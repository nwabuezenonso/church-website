<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Working</title>
</head>
<body>
        <h1>Uploaded data</h1>
        <table border="1">
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Image</th>
            </tr>
            <% item.forEach (function(item) { %>

                <tr>
                    <td><%= item.fname %></td>
                    <td><%= item.lname %></td>
                    <td><img src="<%= item.img %>" width="70" height="70"></td>
                </tr>

            <% }) %> 
        </table>
</body>
</html>