<%- include('./partials/header.ejs') %>
<title><%= title %></title>
<link rel="stylesheet" href="./css/dashboard.css">
</head>
<body>
<%- include('./partials/navboard.ejs') %>


<section>
<div style="margin: 0 0 0 12%;" class="table-responsive text-nowrap">
  <!--Table-->
  <table class="table table-striped">

    <!--Table head-->
    <thead>
      <tr>
        <th>#</th>
        <th>Full Name</th>
        <th>Last Name</th>
        <th>Middle Name</th>
        <th>Address</th>
        <th>Marital Status</th>
        <th>Sex</th>
        <th>Home Address</th>
        <th>City/Town</th>
        <th>Date of birth</th>
        <th>Country</th>
        <th>Zip code</th>
        <th>Email</th>
        <th>Occupation</th>
        <th>Date of birth</th>
        <th>Gender</th>
        <th>Baptized</th>
        <th>Confirmed</th>
        <th>Children Name</th>
        <th>Children Date of Birth</th>
        <th>Gender</th>
        <th>Baptized</th>
        <th>Confirmed</th>
        <th>Ministries interested</th>
        <th>Former Church or Religion</th>
      </tr>
    </thead>
    <!--Table head-->

    <!--Table body-->
    <tbody>
      <tr>
        <% ourMember.forEach(ourMember => { %>
        <th scope="row"></th>
        <td><%= ourMember.fname %></td>
        <td><%= ourMember.lname %></td>
        <td><%= ourMember.mname %></td>
        <td><%= ourMember.address %></td>
        <td><%= ourMember.maritalstatus %></td>
        <td><%= ourMember.sex %></td>
        <td><%= ourMember.homeaddress %></td>
        <td><%= ourMember.City %></td>
        <td><%= ourMember.Dob %></td>
        <td><%= ourMember.country %></td>
        <td><%= ourMember.Zip %></td>
        <td><%= ourMember.email %></td>
        <td><%= ourMember.occupation %></td>
        <td><%= ourMember.gender %></td>
        <td><%= ourMember.Baptized %></td>
        <td><%= ourMember.confirmed %></td>
        <td><%= ourMember.no_of_children %></td>
        <td><%= ourMember.children_name %></td>
        <td><%= ourMember.children_dob %></td>
        <td><%= ourMember.gender_of_children %></td>
        <td><%= ourMember.Baptized_kids %></td>
        <td><%= ourMember.kid_confirmed %></td>
        <td><%= ourMember.interested_ministries %></td>
        <td><%= ourMember.Former_church %></td>
        <%  }) %>
      </tr>
    
    </tbody>
    <!--Table body-->


  </table>
  <!--Table-->
</div>
</section>



</body>
</html>